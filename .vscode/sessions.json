{
    "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
    "theme": "tribe",
    "active": "default",
    "activateOnStartup": true,
    "keepExistingTerminals": false,
    "sessions": {
        "default": [
            {
                "name": "Build Runner (WATCH)",
                "autoExecuteCommands": true,
                "icon": "settings",
                "color": "terminal.ansiBrightGreen",
                "commands": [
                    "echo \"Flutter DEV - Starting Build Runner in Watch Mode\"",
                    "dart run build_runner watch --delete-conflicting-outputs"
                ]
            },
            {
                "name": "User's Terminal",
                "autoExecuteCommands": true,
                "icon": "keyboard",
                "color": "terminal.ansiBrightBlue",
                "commands": [
                    "echo \"Flutter DEV - User's terminal\"",
                    "git status"
                ]
            }
        ]
    }
}